<table class="highlight centered">
  <thead>
    <tr>
      <% build_header(safe_user_attrs) %>
    </tr>
  </thead>
  <tbody>
    <% if @users.length != 0 %>
      <% @users.each do |user| %>
        <tr class="admin-table-row" id=<%= edit_administration_user_path(user) %>>
          <% build_row(user, safe_user_attrs) %>
        </tr>
      <% end %>
    <% else %>
      <%= render 'shared/table_no_records' %>
    <% end %>
  </tbody>
</table>
