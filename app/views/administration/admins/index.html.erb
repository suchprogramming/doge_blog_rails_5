<div class="row">
  <div class="col s12">

    <div class="section content-top-spacer">
      <div class="section-header-bar flex-center">
        <h4>Admin List</h4>
        <%= link_to 'Invites', superadmins_invitations_path, base_button %>
      </div>
    </div>

    <div class="section">
      <div class="card-panel">
        <div class="row no-bottom-margin">
          <div class="col s12">
            <%= render 'shared/admin_search',
                        search_type: 'admin_search',
                        path: administration_admins_path %>
          </div>
        </div>

        <div id="admins">
          <%= render 'admin_table' %>
        </div>
      </div>
    </div>

  </div>
</div>
